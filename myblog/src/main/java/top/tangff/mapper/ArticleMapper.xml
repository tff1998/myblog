  <select id="getArtilcePage" resultMap="articlePage" parameterType="List">
	SELECT c.id, c.title, c.image, c.head, c.created, c.articleCatName, u.name AS useName
  	FROM user u INNER JOIN 
	  	(SELECT a.id, a.use_id, a.title, a.image, a.head, a.created, ac.name AS articleCatName
	     FROM article a INNER JOIN article_cat ac
	  	 ON a.cid = ac.id
