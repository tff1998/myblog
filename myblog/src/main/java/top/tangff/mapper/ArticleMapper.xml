	  	 	 WHERE a.cid IN 
		     <foreach collection="list" item="aticleCatId" index="index"  
	            open="(" close=")" separator=",">  
	            #{aticleCatId}  
	        </foreach> 
		</if>
	  	 ) AS c
	ON u.id = c.use_id
	ORDER BY c.created DESC
  </select>
